{
    "env": {
        "browser": true,
        "es2021": true
    },    
    "parserOptions": {        
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [       
    ],    
    "rules": {
        "no-use-before-define": "off",
        "no-shadow": "off",
        "arrow-body-style": ["error", "as-needed"],
        "no-console": ["error", { "allow": ["log", "debug", "warn", "error"] }],
        "import/prefer-default-export": "off"        
    }
}